{
    "name": "Instagram Downloader",
    "description": "Thanks for installing,",
    "version": "1.0",
    "permissions": [
      "activeTab","tabs"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.instagram.com/*"],
        "js": ["content-script.js"],
        "css" : ["styles.css"]
      }
    ],
    "browser_action": {
      "default_title": "Instagram Downloader",
      "default_popup": "index.html"
    },
    "manifest_version": 2,
    "background": {
      "service_worker": "background.js"
    }
  }